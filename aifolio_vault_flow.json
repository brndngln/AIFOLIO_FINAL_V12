{
  "name": "AIFOLIO_Vault_Automation",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "GET",
        "path": "aifolio-trigger"
      },
      "id": "Webhook",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "url": "http://localhost:8000/generate-vault",
        "method": "GET",
        "responseFormat": "json"
      },
      "id": "HTTPRequest",
      "name": "Call /generate-vault",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [500, 300]
    },
    {
      "parameters": {
        "amount": 5,
        "unit": "seconds"
      },
      "id": "Wait",
      "name": "Wait 5 seconds",
      "type": "n8n-nodes-base.wait",
      "typeVersion": 1,
      "position": [750, 300]
    },
    {
      "parameters": {
        "message": "âœ… AIFOLIO Vault successfully created!",
        "chatId": "YOUR_TELEGRAM_CHAT_ID",
        "additionalFields": {}
      },
      "id": "Telegram",
      "name": "Telegram Alert",
      "type": "n8n-nodes-base.telegram",
      "typeVersion": 1,
      "credentials": {
        "telegramApi": {
          "id": "replace-with-your-token",
          "name": "My Telegram Bot"
        }
      },
      "position": [1000, 300]
    }
  ],
  "connections": {
    "Webhook Trigger": {
      "main": [[{ "node": "Call /generate-vault", "type": "main", "index": 0 }]]
    },
    "Call /generate-vault": {
      "main": [[{ "node": "Wait 5 seconds", "type": "main", "index": 0 }]]
    },
    "Wait 5 seconds": {
      "main": [[{ "node": "Telegram Alert", "type": "main", "index": 0 }]]
    }
  },
  "active": false
}