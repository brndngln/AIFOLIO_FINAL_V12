{
  "name": "Vault Failure Alert",
  "nodes": [
    {
      "parameters": {
        "webhookUri": "/webhook/vault-failure-alert",
        "httpMethod": "POST"
      },
      "name": "Vault Failure Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [200, 300]
    },
    {
      "parameters": {
        "chatId": "@AIFOLIO_ALERTS",
        "message": "[AIFOLIO OMNILOCK] Vault Failure: {{$json[\"msg\"]}}",
        "options": {}
      },
      "name": "Send Telegram Alert",
      "type": "n8n-nodes-base.telegram",
      "typeVersion": 1,
      "position": [400, 300]
    }
  ],
  "connections": {
    "Vault Failure Webhook": {
      "main": [[{"node": "Send Telegram Alert", "type": "main", "index": 0}]]
    }
  }
}
