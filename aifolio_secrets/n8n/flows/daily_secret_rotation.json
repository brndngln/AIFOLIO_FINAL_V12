{
  "name": "AIFOLIO Daily Secret Rotation",
  "nodes": [
    {
      "parameters": {
        "cronExpression": "0 2 * * *"
      },
      "name": "Cron Trigger",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "url": "http://localhost:PORT/secrets/rotate",
        "method": "POST"
      },
      "name": "Trigger Secret Rotation",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [480, 300]
    }
  ],
  "connections": {
    "Cron Trigger": {
      "main": [
        [
          {
            "node": "Trigger Secret Rotation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
